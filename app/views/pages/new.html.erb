<% meta :title => "Andrew Jorczak",
				:description => "Andrew Jorczak's personal site",
				:keywords => %w(rails ruby on rails Rails) 
%>


<%= form_for(@page) do |f| %>

	<p>
		<%= f.label :title %><br/>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :slug %><br/>
		<%= f.text_field  :slug %>
	</p>
	<p>
		<%= f.label :content %> <br/>
		<%= f.text_field :content %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>